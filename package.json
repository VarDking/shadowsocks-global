{
  "name": "shadowsocks-global",
  "version": "0.1.0-beat.8",
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "cap": "^0.2.1",
    "detect-port": "^1.3.0",
    "electron-promise-ipc": "^2.1.0",
    "electron-store": "^5.1.1",
    "geoip-country": "^4.0.9",
    "log4js": "^6.1.2",
    "socks": "^2.3.3",
    "sudo-prompt": "^9.1.1"
  },
  "description": "A VPN or proxy client for Windows.",
  "author": "igoogolx < igoogolx@gmail.com >",
  "license": "MIT",
  "scripts": {
    "start": "concurrently  \"yarn react-start\" \"yarn electron-build:dev\" \"wait-on http://localhost:3000 && yarn electron-start:watch\"",
    "build": "yarn electron-build && yarn react-build && yarn electron-builder && yarn rebuild",
    "react-start": "react-app-rewired start",
    "electron-start:watch": "nodemon --exec npm run electron-start",
    "electron-builder": "electron-builder --win --publish never --config electron/builder.json --config.extraMetadata.name ShadowsocksGlobal",
    "electron-start": "electron .",
    "electron-build": " rollup -c",
    "electron-build:dev": "rollup -c -w --environment IS_DEV",
    "clean": "rimraf electron-build build dist",
    "react-build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "react-analyze": "source-map-explorer 'build/static/js/*.js'",
    "rebuild": "electron-builder install-app-deps",
    "postinstall": "npm run rebuild"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.3.2",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/chart.js": "^2.9.8",
    "@types/classnames": "^2.2.9",
    "@types/detect-port": "^1.1.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/node": "^12.12.26",
    "@types/qrcode": "^1.3.4",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-transition-group": "^4.2.3",
    "@types/react-window": "^1.8.1",
    "@types/uuid": "^7.0.0",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^2.2.4",
    "flag-icon-css": "^3.4.6",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.1",
    "qrcode": "^1.4.4",
    "react": "^16.13.1",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-transition-group": "^4.3.0",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "rollup": "^2.3.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "source-map-explorer": "^2.4.2",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2",
    "wait-on": "^4.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "public/electron.js"
    ],
    "delay": 2000
  }
}
